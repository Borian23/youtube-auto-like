{
  "manifest_version": 2,

  "name": "YouTube Auto Like",
  "description": "Magically likes videos from channels you subscibe to.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs"
  ],

  "content_scripts": [{
    "matches": ["*://youtube.com/watch*v=*", "*://*.youtube.com/watch*v=*"],
    "js": ["like.js"],
    "run_at": "document_end"
  }]
}